{
	"name": "nao4you",
	"version": "1.0.0",
	"scripts": {
		"lint": "biome check .",
		"lint:fix": "biome check --apply .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"postinstall": "git config core.hooksPath .husky"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"biome check --apply",
			"biome format --write",
			"biome check"
		]
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0"
	}
}
